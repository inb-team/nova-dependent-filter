{
    "name": "deployfy/nova-dependent-filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0",
        "laravel/nova-devtool": "^1.7"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "Deployfy\\NovaDependentFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deployfy\\NovaDependentFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
